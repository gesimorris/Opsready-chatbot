Possibly Keep
       Tool(
            name="get_workspace_deficiencies",
            description="Fetches all deficiencies for a given workspace and lists their status.",
            inputSchema={
                "type": "object",
                "properties": {
                    "workspace_name": {
                        "type": "string",
                        "description": "The name of the OpsReady workspace (e.g., 'Summit Base')."
                    },
                    "task_required": {
                        "type": "boolean",
                        "description": "Only return deficiencies with tasks associated to the, also use to check if a deficiency has a task linked to it"
                    },
                    "work_order_required": {
                        "type": "boolean",
                        "description": "Only return deficiencies with work orders linked to it and use to check if a deficiency has a work order linked to it"
                    }
                },
                "required": ["workspace_name"]
            }
        ),


                name_normalized = workspace_name.strip().lower()
        workspace_name_to_loc = {
                "summit base": "SUMMIT_BASE",
                "ridgeview Base": "RIDGE_VIEW"
            }

    # get the location formatted name
        workspace_loc = workspace_name_to_loc.get(name_normalized)
        if workspace_loc:
            return [
                TextContent(
                    type="text",
                    text=f"I don't recognize the workspace '{workspace_name}'."
                        " Try one of: " + ", ".join(workspace_name_to_loc.keys())
                    )
                ]